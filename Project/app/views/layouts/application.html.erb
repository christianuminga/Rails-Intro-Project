<!DOCTYPE html>
<html>
  <head>
    <title>Project</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <header>
  <h1> <%= link_to '311 Service Requests', root_path%> </h1>
  <p><%= link_to 'About Us', about_path %>
    <%= form_tag(results_search_index_path, method: "get") do %>
  <%= label_tag(:q, "Search:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>
  </header>
    <%= yield %>
        <footer>
      <p> Full-Stack Project 2018 </p>
    </footer>
  </body>
</html>
